action code {:
      int prixTotal = 0;
:}

terminal String ASPECTLIEU;
terminal ACTIVITES, ANIMATEUR, PARTICIPANTS, FIN, CENTRE, JOUR, DEUXPOINT, POINT, VIRG, 
         HORAIRE, EGALE, CHAINE, PRENOM, ACCOVT, ACCFMT, POINTVIRG;

non terminal planning, listeJour, listeCreneau,listePrenom;


planning ::= ACTIVITES CENTRE listeJour FIN POINT |   ;

listeJour ::= listeJour JOUR DEUXPOINT listeCreneau 
              | JOUR DEUXPOINT listeCreneau  ;

listeCreneau ::= listeCreneau HORAIRE EGALE CHAINE VIRG ASPECTLIEU  CHAINE VIRG ANIMATEUR 
                    EGALE PRENOM VIRG PARTICIPANTS EGALE ACCOVT listePrenom ACCFMT POINTVIRG
                 | HORAIRE EGALE CHAINE VIRG ASPECTLIEU CHAINE VIRG ANIMATEUR 
                    EGALE PRENOM VIRG PARTICIPANTS EGALE ACCOVT listePrenom ACCFMT POINTVIRG ;

listePrenom ::= PRENOM VIRG listePrenom | PRENOM |   ;
